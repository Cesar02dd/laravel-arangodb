{
  "name": "LaravelFreelancerNL/Aranguent",
  "description": "Laravel bridge for the ArangoDB Multi-model database",
  "keywords": [
    "laravel",
    "arangodb",
    "multi-model",
    "document",
    "graph",
    "database",
    "aranguent"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Laravel Freelancer",
      "email": "info@laravel-freelancer.nl"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "triagens/arangodb": "^3",
    "nesbot/carbon": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "mockery/mockery": "1.2",
    "orchestra/testbench": "^3",
    "doctrine/dbal": "^2"
  },
  "repositories": [ ],
  "autoload": {
    "psr-4": {
      "LaravelFreelancerNL\\Aranguent\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "LaravelFreelancerNL\\Aranguent\\AranguentServiceProvider"
      ]
    }
  }
}