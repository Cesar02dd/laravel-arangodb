{
  "name": "laravel-freelancer-nl/aranguent",
  "description": "Laravel bridge for the ArangoDB Multi-model database",
  "keywords": [
    "laravel",
    "arangodb",
    "multi-model",
    "document",
    "graph",
    "database",
    "aranguent"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Laravel Freelancer",
      "email": "info@laravel-freelancer.nl"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "laravel/framework": "^7",
    "triagens/arangodb": "3.6.*",
    "ext-json": "*",
    "laravel-freelancer-nl/fluentaql": "^1.0"
  },
  "require-dev": {
    "orchestra/testbench": "5.*",
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.3.1",
    "sebastian/phpcpd": "^6.0",
    "squizlabs/php_codesniffer": "^3.0@dev",
    "phpstan/phpstan": "^0.12.0@dev",
    "phpmd/phpmd": "dev-master",
    "nunomaduro/larastan": "0.6.x-dev"
  },
  "repositories": [ ],
  "autoload": {
    "psr-4": {
      "LaravelFreelancerNL\\Aranguent\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "LaravelFreelancerNL\\Aranguent\\AranguentServiceProvider"
      ]
    }
  }
}
